{
  "name": "friendly-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "yaml",
        "path": "./snippets/istio-manifests.json"
      },
      {
        "language": "terraform",
        "path": "./snippets/terraform.json"
      },
      {
        "language": "python",
        "path": "./snippets/python_snippets.json"
      },
      {
        "language": "node",
        "path": "./snippets/node-snippets.json"
      },
      {
        "language": "helm",
        "path": "./snippets/helm.json"
      },
      {
        "language": "yaml",
        "path": "./snippets/kubernetes.json"
      },
      {
        "language": "groovy",
        "path": "./snippets/jenkinsfile.json"
      }
    ]
  }
}
